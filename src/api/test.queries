mutation {
  register(firstName:"bob", lastName: "bob", email: "bob@bob.com", password:"bobbob") {
    errors{
      code
      path
      message
    }
    result {
      user {
        email
        firstName
        userID
      }
      session {
        token
      }
    }
  }
}

mutation {
	login(email:"bob@bob.com", password: "bobbob") {
    error {
      message
      path
      code
    }
    auth {
      user {
        userID
        email
      }
      session {
        token
      }
    }
  }
}

mutation {
  joinGroup(userID: 1, inviteCode:"t9pog") {
    user {
      firstName
      teams {
        group {
          groupName
        }
      }
    }
    group {
      groupName,
      team {
        user {
        	firstName 
        }
      }
    }
    error {
      code
      message
      path
    }
  }
}

query {
	user(userID: 1) {
    userID
  }
}
