mutation {
  register(email:"bob@bob.com", password:"bobbob", firstName:"bob", lastName: "bob") {
    error{
      path
      code
      message
    }
    auth {
      user {
        userID
        email
      }
      session {
        token
      }
    }
  }
}

mutation {
	login(email:"bob@bob.com", password: "bobbob") {
    error {
      message
      path
      code
    }
    auth {
      user {
        userID
        email
      }
      session {
        token
      }
    }
  }
}

query {
	user(userID: 1) {
    userID
  }
}
