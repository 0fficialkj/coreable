{
  "name": "coreable",
  "version": "1.0.1",
  "description": "Coreable Back End GraphQL API",
  "main": "build/server.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "docz:dev": "docz dev --src='./docs' --port=3001",
    "docz:build": "rimraf ./.docz && docz build --src='./docs'",
    "docz:serve": "docz serve",
    "docz:deploy": "",
    "api:dev": "nodemon -e .js,.ts --watch api --exec \"NODE_ENV=development TS_NODE_PROJECT='./api/tsconfig.json' ts-node ./api/server.ts\"",
    "api:build": "rimraf ./build/api && tsc -p ./api/tsconfig.json",
    "api:serve": "NODE_ENV=production node ./build/api/server.js",
    "api:test": "rimraf ./.nyc_output && NODE_ENV=development TS_NODE_PROJECT='./api/tsconfig.spec.json' nyc mocha --recursive --timeout 10000 -r ts-node/register \"api/**/*.spec.ts\"",
    "react:dev": "",
    "react:build": "",
    "react:serve": "",
    "react:test": "",
    "deploy": "npm run api:build && npm run react:build && gcloud app deploy",
    "start": "pm2 start",
    "stop": "pm2 stop all"
  },
  "keywords": [],
  "author": "Jake Walklate [svnty]",
  "license": "Copyright (c) Coreable",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/md5": "^2.1.33",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.2",
    "@types/validator": "^12.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "docz": "^2.3.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "faker": "^4.1.0",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0",
    "pm2": "^4.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^5.21.5",
    "sequelize-simple-cache": "^1.1.1"
  }
}
